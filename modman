app/code/local/Strattas/Core
app/code/local/Strattas/Master
app/design/adminhtml/default/default/layout/strattas_master.xml
app/design/adminhtml/default/default/template/strattas_master
app/design/frontend/default/default/template/strattasmaster
app/etc/modules/Strattas_Core.xml
